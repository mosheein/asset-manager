{
  "name": "asset-manager",
  "version": "1.0.0",
  "description": "Personal portfolio management app for Interactive Brokers statements",
  "main": "dist/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon --exec ts-node src/server.ts",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "cd client && npm run build",
    "start": "node dist/server.js",
    "init:db": "ts-node src/db/init.ts",
    "clear:db": "ts-node src/db/clear.ts",
    "migrate:db": "ts-node src/db/migrate.ts",
    "migrate:currency": "ts-node src/db/migrate-currency.ts",
    "migrate:target-isin": "ts-node src/db/migrate-target-isin.ts",
    "migrate:remove-fk": "ts-node src/db/migrate-remove-fk.ts",
    "migrate:multiple-tickers": "ts-node src/db/migrate-multiple-tickers.ts",
    "migrate:fix-unique": "ts-node src/db/migrate-fix-unique-constraint.ts",
    "migrate:match-type": "ts-node src/db/migrate-match-type.ts"
  },
  "keywords": [
    "portfolio",
    "investment",
    "interactive-brokers"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/pdf-parse": "^1.1.4",
    "@types/xlsx": "^0.0.35",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
